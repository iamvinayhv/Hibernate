package com.utility;

import com.dao.EmployeeDao;
import com.dto.Employee;

public class EmpUtility {

	public static void main(String[] args) {
		
		Employee emp= new Employee();
		emp.setId(1);
		emp.setName("Surya");
		emp.setSalary(20000);
		emp.setDepartment("Java");
		
		EmployeeDao dao=new EmployeeDao();
		dao.insert(emp);
	}

}
