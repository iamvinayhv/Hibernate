package com.bridgelabz.dto;

import javax.persistence.Column;
import javax.persistence.Id;

public class Employee {

	@Id
	@Column(name="ID")
	private int id;
	@Column(name="Emp_Name")
	private String name;
	@Column(name="Salary")
	private int salary;
	@Column(name="Dept")
	private String dept;
	@Column(name="Password")
	private String password;
	
}
